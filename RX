#include <SPI.h>                                                    //library for SPI
#include <RF24.h>                                                   //library for NRF24L01 Radio module
#include <Wire.h>                                                   //library for I2C
#include <Adafruit_GFX.h>                                           //library for Advanced OLED Graphics
#include <Adafruit_SSD1306.h>                                       //library for Using OLED



int SLIDE = 0;                                                      //Declaring SLIDE Variable
float LOWTEMP = 100;                                                //Declaring LOWTEMP Variable
float HIGHTEMP = -100;                                              //Declaring HIGHTEMP Variable


RF24 radio(9, 8);                                                   //CE, CSN                                            
const byte address[6] = "1Node";                                    //Declaring the adress the radio will use
Adafruit_SSD1306 display(128, 64, &Wire, -1);                       //give OLED the required info

struct SensorData {                                                 //create a data package for the radio module
  float TEMP;                                                       //create a floating point variable TEMP in the package
  float HUM;                                                        //create a floating point variable HUM in the package
  float UV;                                                         //create a floating point variable UV in the package

};
SensorData data;                                                    //create a instance of SensorData (data)
