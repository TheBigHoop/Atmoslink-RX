void setup() {                                         
  
  radio.begin();                                                      //Start Radio up
  display.begin(SSD1306_SWITCHCAPVCC, 0x3C);                          //Start OLED up
   

  radio.begin();                                                     // Initialize RF24
  radio.openReadingPipe(1, address);                                 // Open pipe with address
  radio.setPALevel(RF24_PA_LOW);                                     // Set power level
  radio.setDataRate(RF24_1MBPS);                                     // Set data rate 
  radio.startListening();                                            // Declaring Radio as a Receiver

  pinMode(2, INPUT);                                                 // Declaring pin 2 as input
  attachInterrupt(digitalPinToInterrupt(2), Interrupt, FALLING);     // attaching a interrupt to pin 2 
}


void loop() {
  
  if (radio.available()) {                                           // Checking whether there is a radio message available 
    radio.read(&data, sizeof(data));                                 // If a radio messsage is available, then read it and 

    
    if (data.TEMP < LOWTEMP) {                                       // Changes the LOWTEMP to the lowest TEMP yet
      LOWTEMP = data.TEMP;                                           // ^^^^^^
    }
    if (data.TEMP > HIGHTEMP) {                                      // Changes the HIGHTEMP to the HIGHEST TEMP yet
      HIGHTEMP = data.TEMP;                                          // ^^^^^^
    }


    if (SLIDE == 0) {                                               // deciding which data to display ]
      TEMP(data);                                                   //                                ]
    } else if (SLIDE == 1) {                                        //                                ]
      HUM(data);                                                    //                                ]
    } else if (SLIDE == 2) {                                        //                                ]
      UV(data);                                                     //                                ]
    }



    
  
  }
}

void Interrupt(){                                                   // Interrupt function
  SLIDE = (SLIDE+1) % 3;                                            // Adds one to SLIDE until it hits 2 then it returns to 0 making a circle scroll
}

void TEMP(SensorData data) {                                        // Function that displays the required info for Temprature
  display.clearDisplay();                                           // Clears display
  display.setCursor(0, 0);                                          // Sets cursor to top left
  display.setTextSize(1);                                           // Set min text size
  display.setTextColor(WHITE);                                      // Set text color
  display.print("TEMP = ");                                         // Displaying data
  display.print(data.TEMP);                                         // Displaying data
  display.print((char)247);                                         // Degree symbol
  display.print("C       ");                                        // Displaying data
  display.print("HIGH = ");                                         // Displaying data
  display.print(HIGHTEMP);                                          // Displaying data
  display.print((char)247);                                         // Degree symbol
  display.print("C       ");                                        // Displaying data
  display.print("LOW  = ");                                         // Displaying data
  display.print(LOWTEMP);                                           // Displaying data
  display.print((char)247);                                         // Degree symbol
  display.println("C          ");                                   // Displaying data
  display.clearDisplay();                                           // Displaying data
  display.print("Feeling = ");                                      // Displaying data

  if (data.TEMP < 10.00) {
    display.print("FREEZING");                                      // Changing bitmap based on temp
    display.drawBitmap(30, 0, SNOWFLAKE, 100, 32, WHITE);           // Changing bitmap based on temp
  } else if (data.TEMP < 15.00) {                                   // Changing bitmap based on temp
    display.print("VERY COLD");                                     // Changing bitmap based on temp
    display.drawBitmap(30, 0, SNOWFLAKE, 100, 32, WHITE);           // Changing bitmap based on temp
  } else if (data.TEMP < 19.00) {                                   // Changing bitmap based on temp
    display.print("COLD");                                          // Changing bitmap based on temp
    display.drawBitmap(30, 0, SNOWFLAKE, 100, 32, WHITE);           // Changing bitmap based on temp
  } else if (data.TEMP < 26.00) {                                   // Changing bitmap based on temp
    display.print("ROOM TEMP");                                     // Changing bitmap based on temp
    display.drawBitmap(24, -1, ROOMTEMP, 100, 32, WHITE);           // Changing bitmap based on temp
  } else if (data.TEMP < 29.00) {                                   // Changing bitmap based on temp
    display.print("WARM");                                          // Changing bitmap based on temp
    display.drawBitmap(30, -5, SUN, 100, 50, WHITE);                // Changing bitmap based on temp
  } else if (data.TEMP < 34.00) {                                   // Changing bitmap based on temp
    display.print("HOT");                                           // Changing bitmap based on temp
   display.drawBitmap(30, -5, SUN, 100, 50, WHITE);                 // Changing bitmap based on temp
  } else if (data.TEMP > 34.01) {                                   // Changing bitmap based on temp
    display.print("VERY HOT");                                      // Changing bitmap based on temp
    display.drawBitmap(24, -1, SUN, 100, 32, WHITE);                // Changing bitmap based on temp
  }
  
  
  display.display();                                                // Sending all display functions on the bufffer to the screen
}
void HUM(SensorData data) {                                         // Function that displays the required info for Humidity
  display.clearDisplay();                                           // Clears display
  display.setCursor(0, 0);                                          // Sets cursor to top left
  display.setTextSize(1);                                           // Set min text size
  display.setTextColor(WHITE);                                      // Set text color
  display.print("Humidity = ");                                     // Displaying data
  display.print(data.HUM);                                          // Displaying data
  display.print("%    ");                                           // Displaying data

  
  if(data.HUM<20.00){                                               // Changing bitmap based on HUM
    display.print("Dry");                                           // Changing bitmap based on HUM
    display.drawBitmap(0, 30, cactus, 128, 32, WHITE);              // Changing bitmap based on HUM
  }
  else if (data.HUM<50){                                            // Changing bitmap based on HUM
    display.print("Comfortable");                                   // Changing bitmap based on HUM
    display.drawBitmap(0, 30, Rain1, 128, 32, WHITE);               // Changing bitmap based on HUM
  }
  else if (data.HUM<70){                                            // Changing bitmap based on HUM
    display.print("Humid");                                         // Changing bitmap based on HUM
    display.drawBitmap(0, 30, Rain2, 128, 32, WHITE);               // Changing bitmap based on HUM
  }
  else if (data.HUM>70){                                            // Changing bitmap based on HUM
    display.print("Very Humid");                                    // Changing bitmap based on HUM
    display.drawBitmap(0, 30, Rain3, 128, 32, WHITE);               // Changing bitmap based on HUM
  }

  display.display();                                                // Sending all display functions on the bufffer to the screen
}
void UV(SensorData data) {                                          // Function that displays the required info for UV
   display.clearDisplay();                                          // Clears display
  display.setCursor(0, 0);                                          // Sets cursor to top left
  display.setTextSize(1);                                           // Set min text size
  display.setTextColor(WHITE);                                      // Set text color
  display.print("UV = ");                                           // Displaying data
  display.print(data.UV);                                           // Displaying data

  if (data.UV < 2) {                                                // Changing bitmap based on UV
    display.drawBitmap(20, 15, VERYLOWUV, 100, 64, WHITE);          // Changing bitmap based on UV
    
  } else if (data.UV < 4) {                                         // Changing bitmap based on UV
    display.drawBitmap(20, 15, LOWUV, 100, 64, WHITE);              // Changing bitmap based on UV
    
  } else if (data.UV < 6) {                                         // Changing bitmap based on UV
    display.drawBitmap(20, 15, MEDIUMUV, 100, 64, WHITE);           // Changing bitmap based on UV
   
  } else if (data.UV < 8) {                                         // Changing bitmap based on UV
    display.drawBitmap(20, 15, HIGHUV, 100, 64, WHITE);             // Changing bitmap based on UV
   
  } else if (data.UV < 11) {                                        // Changing bitmap based on UV
    display.drawBitmap(20, 15, VERYHIGHUV, 100, 64, WHITE);         // Changing bitmap based on UV
    
  }
  
  display.display();                                                // Sending all display functions on the bufffer to the screen
}
